{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Insumo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Insumo</th>
                <td>{{ entity.insumo }}</td>
            </tr>
            <tr>
                <th>Embalagem</th>
                <td>{{ entity.embalagem }}</td>
            </tr>
            <tr>
                <th>Un</th>
                <td>{{ entity.un }}</td>
            </tr>
            <tr>
                <th>Descricao</th>
                <td>{{ entity.descricao }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('insumo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('insumo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
